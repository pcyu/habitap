extends layoutafter

block content
  div.y-template
    h2 New Habit Entry
    form.new-habit-entry(action='/habits/persons' method='POST')
      input(type='text' name='habitquestion' placeholder='Habit Question' required)
      input(type='text' name='password' placeholder='Password' required)
      input(type='submit' class='login-button' value='post')
